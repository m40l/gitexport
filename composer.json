{
    "name": "msleonar/gitexport",
    "description": "Keeping track of git versions in laravel projects",
    "keywords": ["laravel", "git"],
    "license": "MIT",
    "version": "0.2",
    "authors": [{
        "name": "Matthew Leonard",
        "email": "msleonar@ncsu.edu"
    }],
    "require": {
        "php": ">=7.0"
    },
    "autoload": {
        "psr-4": {
            "Msleonar\\Gitexport\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Msleonar\\Gitexport\\ServiceProvider"
            ]
        }
    },
    "require-dev": {
        "illuminate/support": "5.6.x"
    }
}